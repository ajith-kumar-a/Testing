package ak.cal;

public class CalculatorProject {
	
	public int add(int a, int b) {
		
		return a+b;
	}

	
	public int sub(int a, int b) {
		
		return a-b;
	}

	
	public int mul(int a, int b) {
		
		return a*b;
	}

	
	public int div(int a, int b) {
		
		return a/b;
	}

	
	public double AreaOfCircle(int r) {
		
		return Math.PI*r*r;
	}
	
	public boolean positiveNumber(int i) {
		return true;
	}
	
	public double AreaOfTriangle(double h, double b) {
		
		double result = 0;
		
		return result;
	}
	
	public int square(int s) {
		
		return s*s;
	}
}
